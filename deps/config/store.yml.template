- kind: AKVSecrets
  version: 0.0.1
  name: akv-secrets
  specs:
    vaultName: '{VAULT_NAME}'
    tenantID: '{TENANT_ID}'
    clientID: '{CLIENT_ID}'
    clientSecret: '{SECRET}'
- kind: AKVKeys
  version: 0.0.1
  name: akv-keys
  specs:
    vaultName: '{VAULT_NAME}'
    tenantID: '{TENANT_ID}'
    clientID: '{CLIENT_ID}'
    clientSecret: '{SECRET}'
- kind: HashicorpSecrets
  version: 0.0.1
  name: hashicorp-secrets
  specs:
    mountPoint: secret
    address: http://hashicorp:8200
    token: '{VAULT_TOKEN}'
    namespace: ''
- kind: HashicorpKeys
  version: 0.0.1
  name: hashicorp-keys
  specs:
    mountPoint: orchestrate
    address: http://hashicorp:8200
    tokenPath: '{VAULT_TOKEN_PATH}'
    namespace: ''
- kind: AWSSecrets
  version: 0.0.1
  name: aws-secrets
  specs:
    accessID: '{AWS_ACCESS_ID}'
    secretKey: '{AWS_SECRET_KEY}'
    region: '{AWS_REGION}'
    debug: false
- kind: AWSKeys
  version: 0.0.1
  name: aws-keys
  specs:
    accessID: '{AWS_ACCESS_ID}'
    secretKey: '{AWS_SECRET_KEY}'
    region: '{AWS_REGION}'
    debug: false
- kind: Eth1Account
  version: 0.0.1
  name: eth1-accounts
  specs:
    keystore: HashicorpKeys
    specs:
      mountPoint: secret
	  address: http://hashicorp:8200
	  token: '{VAULT_TOKEN}'
	  namespace: ''
